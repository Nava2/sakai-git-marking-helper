{
  "name": "sakai-gh-helper",
  "version": "1.0.0",
  "description": "Marking helper for Github projects downloaded from Sakai.",
  "main": "app.js",
  "scripts": {
    "dbg-download": "node ./node_modules/gulp-cli/bin/gulp && node %NODE_DEBUG_OPTION% ./dist/app.js --download",
    "download": "node ./node_modules/gulp-cli/bin/gulp && node ./dist/app.js --download",
    "dbg-submit": "node ./node_modules/gulp-cli/bin/gulp && node %NODE_DEBUG_OPTION% ./dist/app.js --submit",
    "submit": "node ./node_modules/gulp-cli/bin/gulp && node ./dist/app.js --submit",
    "dbg-package": "node ./node_modules/gulp-cli/bin/gulp && node %NODE_DEBUG_OPTION% ./dist/app.js --package",
    "package": "node ./node_modules/gulp-cli/bin/gulp && node ./dist/app.js --package"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Nava2/sakai-github-education-marking-helper.git"
  },
  "keywords": [
    "sakai",
    "github",
    "git"
  ],
  "author": "Kevin Brightwell <kevin.brightwell2@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nava2/sakai-github-education-clone-all/issues"
  },
  "homepage": "https://github.com/Nava2/sakai-github-education-clone-all#readme",
  "dependencies": {
    "any-promise": "^1.3.0",
    "bluebird": "^3.4.7",
    "glob": "^7.1.1",
    "glob-promise": "^3.1.0",
    "handlebars": "^4.0.6",
    "jszip": "^3.1.3",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "mz": "^2.6.0",
    "simple-git": "^1.65.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/glob": "^5.0.30",
    "@types/handlebars": "^4.0.31",
    "@types/jszip": "^0.0.32",
    "@types/lodash": "^4.14.50",
    "@types/minimist": "^1.2.0",
    "@types/moment-timezone": "^0.2.34",
    "@types/mz": "^0.0.30",
    "@types/node": "^7.0.4",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-load-plugins": "^1.4.0",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-typescript": "^3.1.4",
    "pump": "^1.0.2",
    "typescript": "^2.1.5"
  }
}
